node {

    stage('Checkout'){
        checkout scm
    }

    stage('Build api.v2') {
        sh 'mvn api\pom.xml clean install' 
    }

    stage('Test') {
        echo "Here is testing."
    }

    stage('Deploy') {
        echo "${env.WORKSPACE}"
        bat 'D:\\Jenkins\\workspace\\iGrocery\\devops\\deploy.igrocery.bat'
    }

}
